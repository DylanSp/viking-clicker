{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "forin": false,
        "interface-name": [true, "never-prefix"],
        "jsx-no-multiline-js": false,
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false
    },
    "rulesDirectory": []
}